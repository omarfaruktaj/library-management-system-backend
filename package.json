{
  "name": "library-management-system",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "build": "tsc && tsc-alias ",
    "dev": "ts-node-dev --env-file=.env -r tsconfig-paths/register  src/server.ts",
    "start": "node --env-file=.env dist/server.js",
    "format": "biome format --write src",
    "lint": "biome lint --write  src",
    "lint:fix": "biome lint  --write --unsafe src",
    "format-lint": "biome check --write src",
    "postinstall": "husky"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "envalid": "^8.0.0",
    "express": "^5.0.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "qs": "^6.13.0",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "winston": "^3.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1",
    "@types/node": "^22.9.0",
    "@types/qs": "^6",
    "husky": "^9.1.6",
    "prisma": "^5.22.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
